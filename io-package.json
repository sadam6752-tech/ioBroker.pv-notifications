{
  "_id": "iobroker.pv-notifications",
  "version": "1.0.4",
  "common": {
    "name": "pv-notifications",
    "version": "1.0.4",
    "title": "PV Notifications",
    "titleLang": {
      "en": "PV Notifications",
      "de": "PV-Benachrichtigungen",
      "ru": "PV-Уведомления"
    },
    "desc": {
      "en": "Send Telegram notifications for PV battery status",
      "de": "Sende Telegram-Benachrichtigungen für PV-Batteriestatus"
    },
    "authors": ["Alex <alex@example.com>"],
    "keywords": ["PV", "Solar", "Batterie", "Telegram"],
    "license": "MIT",
    "platform": "Node.js",
    "main": "main.js",
    "mode": "daemon",
    "type": "energy",
    "enabled": true,
    "compact": true,
    "connectionType": "local",
    "dataSource": "push",
    "adminUI": {
      "config": "json"
    },
    "extIcon": "https://raw.githubusercontent.com/sadam6752-tech/ioBroker.pv-notifications/main/admin/pv-notifications.png",
    "icon": "pv-notifications.png",
    "readme": "https://github.com/sadam6752-tech/ioBroker.pv-notifications/blob/main/README.md",
    "loglevel": "info",
    "tier": 3,
    "dependencies": [
      {
        "js-controller": ">=5.0.0"
      }
    ],
    "globalDependencies": [
      {
        "admin": ">=5.0.0"
      }
    ]
  },
  "native": {
    "telegramInstance": "telegram.0",
    "telegramUsers": "",
    "batterySOC": "",
    "powerProduction": "",
    "totalProduction": "",
    "feedIn": "",
    "consumption": "",
    "gridPower": "",
    "batteryCapacityWh": 21000,
    "thresholdFull": 100,
    "thresholdEmpty": 0,
    "thresholdResetFull": 95,
    "thresholdResetEmpty": 5,
    "nightModeEnabled": true,
    "intermediateSteps": "20,40,60,80",
    "minIntervalFull": 10,
    "minIntervalEmpty": 5,
    "minIntervalIntermediate": 30,
    "statsDayTime": "22:00",
    "statsWeekDay": 6,
    "statsWeekTime": "10:00",
    "weatherToday": "",
    "weatherTomorrow": "",
    "highProduction": 5000,
    "highConsumption": 3000,
    "monthlyStatsEnabled": false,
    "monthlyStatsDay": 1,
    "monthlyStatsTime": "09:00",
    "monthlyProduction": "",
    "monthlyConsumption": "",
    "monthlyFeedIn": "",
    "monthlyGridPower": ""
  },
  "objects": [
    {
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Information"
      }
    },
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "name": "Adapter ist mit Telegram verbunden",
        "type": "boolean",
        "role": "indicator.connected",
        "read": true,
        "write": false,
        "def": false
      }
    }
  ],
  "stateRights": true
}
