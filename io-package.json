    "authors": ["Alex <alex@example.com>"],
    "keywords": ["PV", "Solar", "Batterie", "Telegram"],
    "license": "MIT",
    "platform": "Node.js",
    "main": "main.js",
    "mode": "daemon",
    "type": "energy",
    "enabled": true,
    "compact": true,
    "connectionType": "local",
    "dataSource": "push",
    "adminUI": {
      "config": "json"
    },
    "i18n": true,
    "extIcon": "https://raw.githubusercontent.com/sadam6752-tech/ioBroker.pv-notifications/main/admin/pv-notifications.png",
